{
    "name": "ellipse/container-reflection",
    "type": "library",
    "description": "Psr-11 container decorator providing autowiring feature",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection",
        "decorator",
        "autowiring",
        "callable-dependency-injection",
        "runtime-dependency-injection"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/container-reflection",
    "support": {
        "issues": "https://github.com/ellipsephp/container-reflection/issues",
        "source": "https://github.com/ellipsephp/container-reflection"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "mindplay/readable": "^1.1"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Container\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
